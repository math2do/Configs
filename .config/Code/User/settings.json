{
  "editor.fontFamily": "'Source Code Pro','Cascadia Code','Fira Mono','Consolas','JetBrains Mono NL'",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.enablePreview": false,
  "go.toolsManagement.autoUpdate": true,
  "editor.accessibilitySupport": "off",
  "workbench.colorTheme": "Solarized Light",
  "html.autoClosingTags": true,
  "html.format.wrapLineLength": 180,
  "html.format.maxPreserveNewLines": 1,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.ejs": "html",
    ".stylelintrc": "json",
    ".postcssrc": "json",
    ".htmlhintrc": "json",
    ".eslintrc": "json",
    "*.env": "env",
    "*.env.local": "env"
  },
  "css.lint.zeroUnits": "warning",
  "css.lint.emptyRules": "ignore",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.zeroUnits": "warning",
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "javascript.referencesCodeLens.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.format.enable": true,
  "javascript.format.insertSpaceAfterConstructor": false,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "javascript.preferences.quoteStyle": "single",
  "eslint.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onType",
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "emmet.showSuggestionsAsSnippets": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[python]": {
    "editor.formatOnType": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {},
  "editor.language.brackets": [],
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "expand",
  "editor.guides.bracketPairs": "active",
  "editor.fontSize": 16,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[env]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 0, AlignAfterOpenBracket: DontAlign}",
  "code-runner.clearPreviousOutput": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "python": "python3",
    "go": "go run",
    "c": "gcc $fileName -o sol && ./sol < in",
    "cpp": "g++ $fileName -std=c++20 -O2 -o sol -Wall -Wextra -DLOCAL",
    "cpp2": "g++ $fileName -Wall -Wextra -pedantic -std=c++20 -O2 -o sol -Wshadow -Wformat=2 -Wfloat-equal -Wconversion -Wlogical-op -Wshift-overflow=2 -Wduplicated-cond -Wcast-qual -Wcast-align -D_GLIBCXX_DEBUG -D_GLIBCXX_DEBUG_PEDANTIC -D_FORTIFY_SOURCE=2 -fsanitize=address -fsanitize=undefined -fno-sanitize-recover -fstack-protector -DLOCAL"
  },
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-linux": "/home/math2do/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
    "vscode-kubernetes.minikube-path-linux": "/home/math2do/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube"
  }
}